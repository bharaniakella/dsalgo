package sparta;

public class Sparta {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			
			int a[]= {30,30,30,30};
			
			System.out.println("initial size:"+a.length);
			
			int s=1; 
			for(int i=1;i<a.length;i++) {
				if(a[i]!=a[i-1]) {
					s++;
				}
			}
			
			System.out.println("\nsize after removing duplicates:"+s);
			
			
		}
		
	
}
	

