package sparta;

public class Sparta {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int x=20;
		int i=1;
		long fact=1;
		while(i<=x) {
			fact=fact*i;
			i++;
		}
		System.out.println(fact);
		
		int c=0;
		while(fact%5==0) {
			c++;
			fact=fact/5;
		}
		System.out.println(c);
	}
	

}
