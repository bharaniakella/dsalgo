package sparta;

public class Sparta {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int x=94949;
		int temp=x;
		int rev=0;
		
		while(temp!=0) {
			int r=temp%10;
			rev=rev*10+r;
			temp=temp/10;
		}
		System.out.println(rev);
		if(rev==x) {
			System.out.println("palindrome");
		}else {
			System.out.println("not palindrome");
		}
	}
	


}
